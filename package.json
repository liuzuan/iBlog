{
  "name": "iblog",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "debug": "cross-env NODE_ENV=development nodemon src/app.ts",
    "prodebug": "cross-env NODE_ENV=production nodemon src/app.ts",
    "build": "rm -rf dist/ && tsc --project tsconfig.json",
    "dev": "cross-env NODE_ENV=development pm2 start dist/app.js --name 'iBlog' --watch='dist' --node-args='--harmony'",
    "start": "cross-env NODE_ENV=production pm2 start dist/app.js --name 'iBlog' --watch='dist' --node-args='--harmony'",
    "pull": "pm2 start auto_pull.js --name 'auto_pull'",
    "restart": "cross-env NODE_ENV=production pm2 restart dist/app.js --name 'iBlog' --watch='dist' --node-args='--harmony'"
  },
  "dependencies": {
    "@types/express": "^4.16.0",
    "@types/formidable": "^1.0.31",
    "@types/marked": "^0.4.2",
    "@types/mongodb": "^3.1.14",
    "@types/mongoose": "^5.3.1",
    "@types/node": "^10.12.2",
    "@types/prismjs": "^1.9.0",
    "@types/qiniu": "^7.0.1",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.24.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.22.0",
    "babel-register": "^6.24.0",
    "browser-sync": "^2.24.7",
    "config-lite": "^2.1.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "cross-env": "^5.2.0",
    "debug": "~2.6.9",
    "express": "^4.16.4",
    "formidable": "^1.2.1",
    "github-webhook-handler": "^0.7.1",
    "hiredis": "^0.5.0",
    "http-errors": "~1.6.2",
    "jsonwebtoken": "^8.3.0",
    "less-plugin-npm-import": "^2.1.0",
    "marked": "^0.6.2",
    "mongoose": "^5.5.1",
    "morgan": "~1.9.0",
    "multer": "^1.3.1",
    "nodemon": "^1.17.5",
    "prismjs": "^1.15.0",
    "pug": "2.0.0-beta11",
    "qiniu": "^7.2.1",
    "redis": "^2.8.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  },
  "devDependencies": {
    "chalk": "^2.4.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-babel": "^7.0.1",
    "gulp-base64": "^0.1.3",
    "gulp-concat": "^2.6.1",
    "gulp-less": "^3.5.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-minify-css": "^1.2.4",
    "gulp-uglify": "^3.0.0",
    "less-import-aliases": "^1.0.2"
  }
}
