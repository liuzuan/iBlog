{
  "name": "iblog",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development pm2 start start.js --watch --name 'iBlog --port 3000' --node-args='--harmony'",
    "start": "cross-env NODE_ENV=production pm2 start start.js --watch --name 'iBlog --port 3000' --exec --node-args='--harmony'",
    "local": "cross-env NODE_ENV=local pm2 start start.js --name 'iBlog --port 3000' --exec --node-args='--harmony'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "stop": "cross-env NODE_ENV=production pm2 stop start.js --name 'iBlog --port 3000' --exec --node-args='--harmony'",
    "restart": "cross-env NODE_ENV=production pm2 restart start.js --watch --name 'iBlog --port 3000' --exec --node-args='--harmony'"
  },
  "dependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "cookie-parser": "~1.4.3",
    "cross-env": "^5.2.0",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "mongoose": "^5.1.7",
    "morgan": "~1.9.0",
    "nodemon": "^1.17.5",
    "pug": "2.0.0-beta11"
  },
  "devDependencies": {
    "chalk": "^2.4.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-base64": "^0.1.3",
    "gulp-concat": "^2.6.1",
    "gulp-less": "^3.5.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-minify-css": "^1.2.4",
    "gulp-uglify": "^3.0.0",
    "less-import-aliases": "^1.0.2",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0"
  }
}
