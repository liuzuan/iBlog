{
  "name": "iblog",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development pm2 start index.js --watch --name 'iBlog --port 3000'",
    "local": "cross-env NODE_ENV=local pm2 start index.js --name 'iBlog --port 3000'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cross-env NODE_ENV=production pm2 start index.js --watch --node-args='--harmony' --name 'iBlog --port 3000'",
    "stop": "cross-env NODE_ENV=production pm2 stop index.js --name 'iBlog --port 3000'",
    "restart": "cross-env NODE_ENV=production pm2 restart index.js --watch --node-args='--harmony' --name 'iBlog --port 3000'"
  },
  "dependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-3": "^6.22.0",
    "babel-register": "^6.24.0",
    "cookie-parser": "~1.4.3",
    "cross-env": "^5.2.0",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "mongodb": "^3.1.0",
    "mongoose": "^5.1.7",
    "morgan": "~1.9.0",
    "pug": "2.0.0-beta11"
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-base64": "^0.1.3",
    "gulp-concat": "^2.6.1",
    "gulp-less": "^3.5.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-minify-css": "^1.2.4",
    "gulp-uglify": "^3.0.0",
    "less-import-aliases": "^1.0.2"
  }
}
